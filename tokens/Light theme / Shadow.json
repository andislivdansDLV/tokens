{
  "shadow": {
    "opacity": {
      "1": {
        "value": "0.2",
        "type": "other"
      },
      "2": {
        "value": "roundTo({shadow.opacity.1} / {shadow.opacity.scale} /0.01, 0) *0.01",
        "type": "other"
      },
      "3": {
        "value": "roundTo({shadow.opacity.2} / (2*{shadow.opacity.scale}) /0.01, 0) *0.01",
        "type": "other"
      },
      "scale": {
        "value": "1.125",
        "type": "other"
      }
    },
    "color": {
      "value": "{neutral.black}",
      "type": "other"
    }
  }
}